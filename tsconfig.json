{
  "compilerOptions": {
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
, "components/auth/auth-provider.js", "components/auth/reset-password-form.js", "components/auth/signin-form.js", "components/auth/signup-form.js", "app/auth/reset-password/page.js", "app/auth/signin/page.js", "app/auth/signup/page.js"  ],
  "exclude": [
    "node_modules"
  ]
}
